{# 
/**
 * Custom template for iterating over embedded video links
 * 
 */
 #}
{# <div {{ attributes }}> #}
  {% for item in items %}
    <div class="soundcloud-embedded">
      <iframe
        width="100%" height="350"
        src="{{ item.content['#url' ]}}"
        scrolling="no"
        frameborder="no"
        allow="autoplay">
      </iframe>
    </div>
  {% endfor %}
{# </div> #}
