{# 
/**
 * Custom template for iterating over embedded video links
 * 
 */
 #}
<div {{ attributes }}>
  {% for item in items %}
    <div class="video-embedded">
      <iframe
        width="560" height="450"
        src="{{ item.content['#url' ]}}"
        scrolling="no"
        frameborder="no"
        allow="autoplay">
      </iframe>
    </div>
  {% endfor %}
</div>
