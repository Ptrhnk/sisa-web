{# 
/**
 * Custom template for iterating over embedded video links
 * 
 */
 #}
{# <div {{ attributes }}> #}
  {% for item in items %}
    <div class="soundcloud-embedded">
      <iframe
        src="{{ item.content['#url'] }}"
        width="300" height="380"
        frameborder="0"
        allowtransparency="true"
        allow="encrypted-media">
      </iframe>
    </div>
  {% endfor %}
{# </div> #}
